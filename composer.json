{
    "name": "p3k/picofeed",
    "description": "Modern library to handle RSS/Atom feeds",
    "homepage": "https://github.com/aaronpk/picoFeed",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Guillot"
        }
    ],
    "replace": {
        "miniflux/picofeed": "0.1.35"
    },
    "require": {
        "php": ">=7.2",
        "ext-iconv": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-SimpleXML": "*",
        "laminas/laminas-xml": "^1.2"
    },
    "require-dev" : {
        "phpdocumentor/reflection-docblock": "^2.0.4",
        "symfony/yaml": "^2.8.7",
        "phpunit/phpunit" : "^8.5"
    },
    "suggest": {
        "ext-curl": "PicoFeed will use cURL if present"
    },
    "autoload": {
        "psr-4": {"PicoFeed\\": "lib/PicoFeed"}
    },
    "autoload-dev": {
        "psr-4": {"PicoFeed\\": "tests/"}
    },
    "bin" : [
        "picofeed"
    ]
}
